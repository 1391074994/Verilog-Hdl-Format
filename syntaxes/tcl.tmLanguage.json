{
  "name": "TCL",
  "scopeName": "source.tcl",
  "fileTypes": ["tcl"],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#variables"
    },
    {
      "include": "#commands"
    }
  ],
  "repository": {
    "comments": {
      "begin": "#",
      "end": "$",
      "name": "comment.line.number-sign.tcl"
    },
    "strings": {
      "begin": "\"",
      "end": "\"",
      "name": "string.quoted.double.tcl"
    },
    "variables": {
      "match": "\\$\\w+",
      "name": "variable.other.tcl"
    },
    "commands": {
      "match": "\\b(open_hw|set_property|after|append|apply|array|auto_execok|auto_import|auto_load|auto_mkindex|auto_qualify|auto_reset|bgerror|binary|bind|break|case|catch|cd|chan|clock|close|concat|continue|coro|coroutine|dict|encoding|eof|error|eval|exec|exit|expr|fblocked|fconfigure|fcopy|file|fileevent|flush|for|foreach|format|gets|glob|global|history|if|incr|info|interp|join|lappend|lassign|lindex|linsert|list|llength|load|loadTk|lrange|lreplace|lreverse|lsearch|lset|lsort|mathfunc|memory|msgcat|namespace|open|package|pid|pkg::create|pkg_mkIndex|platform|proc|puts|pwd|re_syntax|read|regexp|registry|regsub|rename|resource|return|safe|scan|seek|set|socket|source|split|string|subst|switch|tclLog|tclMacPkgSearch|tclPkgSetup|tclPkgUnknown|tcl_platform|tcl_precision|tcl_rcFileName|tcl_startOfNextWord|tcl_wordBreakAfter|tcltest|tell|throw|time|trace|try|unknown|unset|update|uplevel|upvar|variable|vwait|while|yield)\\b",
      "name": "keyword.control.tcl"
    }
  }
}
